{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2016",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "express", "cors", "cookie-parser", "jsonwebtoken"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
